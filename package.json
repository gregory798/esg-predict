{
	"name": "pinx-nuxt-kit",
	"type": "module",
	"version": "1.15.0",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "nuxt dev",
		"build": "nuxt build",
		"build-check": "run-s libs-reload build lint type-check format test:e2e test:unit",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"test:unit": "vitest",
		"test:e2e": "start-server-and-test preview http://localhost:3000/ 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test preview http://localhost:3000/ 'cypress open --e2e'",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"tailwind-config-viewer": "tailwind-config-viewer -o",
		"design-tokens": "node scripts/tokens-tool.js",
		"libs-check": "taze",
		"libs-reload": "rm -rf node_modules package-lock.json && npm install",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@fontsource/jetbrains-mono": "^5.1.1",
		"@fontsource/lexend": "^5.1.1",
		"@fontsource/public-sans": "^5.1.1",
		"@popperjs/core": "^2.11.8",
		"@tailwindcss/container-queries": "^0.1.1",
		"@vueuse/components": "^11.2.0",
		"@vueuse/core": "^11.2.0",
		"colord": "^2.9.3",
		"dayjs": "^1.11.13",
		"highlight.js": "^11.10.0",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.40.1",
		"pinia": "^2.2.6",
		"pinia-plugin-persistedstate": "^4.1.2",
		"vue": "^3.5.12",
		"vue-advanced-cropper": "^2.8.9",
		"vue-highlight-words": "^3.0.1",
		"vue-router": "^4.4.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^3.8.0",
		"@clack/prompts": "^0.7.0",
		"@css-render/vue3-ssr": "^0.15.14",
		"@iconify/vue": "^4.1.2",
		"@nuxt/devtools": "^1.6.0",
		"@nuxt/eslint": "^0.6.1",
		"@nuxt/test-utils": "^3.14.4",
		"@nuxtjs/device": "^3.2.4",
		"@nuxtjs/i18n": "8.5.6",
		"@nuxtjs/tailwindcss": "^6.12.2",
		"@pinia/nuxt": "^0.7.0",
		"@types/dom-view-transitions": "^1.0.5",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.7",
		"@types/jsdom": "^21.1.7",
		"@types/lodash": "^4.17.13",
		"@types/node": "^22.9.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^10.4.20",
		"cypress": "^13.15.2",
		"eslint": "^9.14.0",
		"fs-extra": "^11.2.0",
		"jsdom": "^25.0.1",
		"npm-run-all2": "^7.0.1",
		"nuxt": "^3.14.159",
		"nuxt-svgo": "^4.0.8",
		"nuxtjs-naive-ui": "^1.0.2",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"sass": "^1.80.6",
		"start-server-and-test": "^2.0.8",
		"tailwind-config-viewer": "^2.0.4",
		"tailwindcss": "^3.4.14",
		"taze": "^0.18.0",
		"ts-node": "^10.9.2",
		"type-fest": "^4.26.1",
		"typescript": "~5.6.3",
		"vitest": "^2.1.4",
		"vue-tsc": "^2.1.10"
	},
	"pnpm": {
		"overrides": {
			"@nuxt/test-utils>jsdom": "$jsdom",
			"@typescript-eslint/eslint-plugin": "^8.13.0",
			"@typescript-eslint/eslint-plugin>eslint": "$eslint",
			"@typescript-eslint/parser": "^8.13.0",
			"@typescript-eslint/parser>eslint": "$eslint",
			"eslint": "$eslint"
		}
	},
	"overrides": {
		"@nuxt/kit": "^3.14.159",
		"@nuxt/schema": "^3.14.159",
		"@nuxt/test-utils": {
			"happy-dom": "15.0.0"
		},
		"@typescript-eslint/eslint-plugin": {
			"eslint": "^9.14.0"
		},
		"@typescript-eslint/parser": {
			"eslint": "^9.14.0"
		},
		"@typescript-eslint/typescript-estree": "^8.13.0"
	},
	"resolutions": {
		"@typescript-eslint/typescript-estree": "^8.13.0"
	}
}